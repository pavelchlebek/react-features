{
  "compilerOptions": {
    "baseUrl": "./src",
    "checkJs": false,
    "jsx": "react",
    "paths": {
      "-app/*": ["src/app/*"],
      "-components/*": ["src/components/*"],
      "-hooks/*": ["src/hooks/*"],
      "-navigation/*": ["src/navigation/*"],
      "-screens/*": ["src/screens/*"],
      "-tools/*": ["src/tools/*"],
      "-styles/*": ["src/styles/*"],
      "-storybook/*": ["src/storybook/*"],
      "-app.config": ["./app.config"]
    }
  }
}
